     color:rgb(49,112,174);
 }

 QListView::item:hover {
     background: rgb(241,249,255);
 }


QScrollBar:vertical {
      border-left: 3px solid rgba(0,0,0,0);
      border-right: 1px solid rgba(0,0,0,0);
      background: rgba(0,0,0,0);
      width: 13px;
      margin: 8px 0 8px 0;
  }
  QScrollBar::handle:vertical:!hover {
      background: rgb(207,230,245);
      min-height: 40px;

  }
  QScrollBar::handle:vertical:hover {
      background: rgb(172,204,225);
      min-height: 40px;

  }
   QScrollBar::handle:vertical:pressed {
      background: rgb(172,204,225);
      min-height: 40px;

  }
  QScrollBar::add-line:vertical {
      border: 0px solid grey;
      background: rgba(0,0,0,0);
      height: 8px;
      subcontrol-position: bottom;
      subcontrol-origin: margin;
  }

  QScrollBar::sub-line:vertical {
      margin:0px;
      border: 0px solid grey;
      background: rgba(0,0,0,0);
      height: 8px;
      subcontrol-position: top;
      subcontrol-origin: margin;
  }
  QScrollBar::up-arrow:vertical:!hover {
	border:0px solid;
	width:13px;
	height:8px;
        background-image:url(:/image/scrollbar_button_up_1.png);
  }
   QScrollBar::up-arrow:vertical:hover {
	border:0px solid;
	margin:0px;
	width:13px;
	height:8px;
      background-image:url(:/image/scrollbar_button_up_2.png);
  }
   QScrollBar::down-arrow:vertical:!hover{
	border:0px solid;
	margin:0px;
	width:13px;
	height:8px;
	background-image:url(:/image/scrollbar_button_down_1.png);
   }
  QScrollBar::down-arrow:vertical:hover{
	border:0px solid;
	margin:0px;
	width:13px;
	height:8px;
	background-image:url(:/image/scrollbar_button_down_2.png);
   }


  QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
      background: none;
  }

