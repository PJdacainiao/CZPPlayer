
QListView::item{
	height:24px;
}
QListView {
     alternate-background-color: rgb(2,2,255);
     background: rgb(230,242,255);
 }

QListView::item:focus{padding:-2;}
QListView::item:selected {
	color:rgb(49,112,174);
     border:1px solid rgb(49,112,174);
     background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                 stop: 0 rgb(153,209,252), stop: 1 rgb(223,239,254));
 }

 QListView::item:selected:!active {
	color:rgb(49,112,174);
      border:1px solid rgb(49,112,174);
     background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                 stop: 0 rgb(153,209,252), stop: 1 rgb(223,239,254));
    
 }

 QListView::item:selected:active {
      border:1px solid rgb(49,112,174);
     background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                 stop: 0 rgb(153,209,252), stop: 1 rgb(223,239,254));
     color:rgb(49,112,174);
 }

 QListWidget::item{
	height:24px;
}
QListWidget {
     alternate-background-color: rgb(241,249,255);
     background: rgb(230,242,255);
 }

QListWidget::item:focus{padding:-2;}
QListWidget::item:selected {
	color:rgb(49,112,174);
